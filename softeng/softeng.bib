	Issn = {0164-1212},
	Journal = {Journal of Systems and Software},
	Number = {1},
	Pages = {9 - 18},
	Title = {Experimental evaluation in computer science: A quantitative study},
	Url = {http://www.sciencedirect.com/science/article/B6V0N-3YGV297-M/2/34bc41d98f4c5c8f206ac42e19c320b3},
